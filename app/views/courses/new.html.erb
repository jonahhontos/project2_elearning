<h1>New Course</h1>
<%= form_for @course do |f| %>
  <%= f.text_field :name, :placeholder => "Course Name" %><br>
  [subject selector]<br>
  <%= f.text_field :description , :placeholder => "Course Description"%><br>
  <%= f.submit %>
<% end %>
<%= link_to "Back", instructor_path(Instructor.find(1)) %>
